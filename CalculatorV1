public class Calculator {
	public static void main (String [] args) {
		int x= Integer.parseInt(args[0]);
		int y= Integer.parseInt(args[1]);
		int z= Integer.parseInt(args[2]);
		String operant = args[3];
		if (operant.equals("+") ) {
			System.out.println(x+y+z);	
		}
		if (operant.equals("-") ) {
			System.out.println(x-y-z);	
		}
	}
}
